<div id="external_login">
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to image_tag("facebook-login.png", :alt => "Sign in with #{provider.to_s.titleize}"), omniauth_authorize_path(resource_name, provider), :class => "#{provider}_login" %>
  <% end -%>
<% end -%>
</div>
