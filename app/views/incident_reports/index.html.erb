<% content_for :head do %>
	<%= auto_discovery_link_tag :atom, incident_reports_url(:atom) %>
	<%= auto_discovery_link_tag :rss, incident_reports_url(:rss) %>
	
	<%= javascript_include_tag 'gmap' %>
	<script type="text/javascript"
	        src="http://maps.google.com/maps/api/js?sensor=false">
	</script>
<% end %>

<div id="map" style="background: #ccc; width:929px; height: 600px;"></div>
<script type="text/javascript">
    a = <%= prepare_markers(@incident_reports); %>
		$(document).ready(function(){
	     static_gmap_with_markers("map", 46.62794, 14.30899, a);
		});
</script>
