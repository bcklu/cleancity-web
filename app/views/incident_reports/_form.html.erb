<%= semantic_form_for(@current_object, :html => { :multipart => true }) do |form| %>
  <%= form.inputs do %>
    <%= form.input :description %>
    <%= form.input :latitude %>
    <%= form.input :longitude %>
	<%= form.input :user_name %>
	<%= form.input :image, :as => :file %>
  <% end %>

  <%= form.buttons %> or <%= link_to 'cancel', @current_object %>
<% end %>
