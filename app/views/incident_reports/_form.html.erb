<%= semantic_form_for(@incident_report, :html => { :multipart => true }) do |form| %>
  <%= form.inputs do %>
    <%= form.input :description %>
    <%= form.input :latitude %>
    <%= form.input :longitude %>
	<%= form.input :user_name %>
	<%= form.input :image, :as => :file %>
  <% end %>

  <%= form.buttons %>
<% end %>
